{
  "env": {
    "amd": true,
    "browser": true
  },
  "globals": {
    "opera": true
  },
  "root": true,
  "rules": {
    "arrow-spacing": [
      "error"
    ],
    "comma-spacing": [
      "error"
    ],
    "curly": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "smart"
    ],
    "indent": [
      "warn",
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "keyword-spacing": [
      "error"
    ],
    "no-eq-null": [
      "error"
    ],
    "no-unused-expressions": [
      "error"
    ],
    "no-caller": [
      "error"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always",
      {
        "omitLastInOneLineBlock": false
      }
    ],
    "space-before-blocks": [
      "error",
      "always"
    ],
    "space-infix-ops": [
      "error"
    ],
    "spaced-comment": [
      "error",
      "always"
    ],
    "strict": [
      "error",
      "safe"
    ],
    "no-undef": [
      "error"
    ],
    "no-unused-vars": [
      "error"
    ],
    "no-eval": [
      "error"
    ]
  }
}